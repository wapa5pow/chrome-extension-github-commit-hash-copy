{
  "name": "GitHub commit hash copy",
  "description": "Show icon on GitHub to copy commit hash to clipboard",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "src/options.html",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}